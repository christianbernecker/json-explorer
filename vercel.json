{
  "version": 2,
  "routes": [
    { 
      "src": "/json-explorer", 
      "dest": "/json-explorer/index.html" 
    },
    { 
      "src": "/json-explorer/(.*)", 
      "dest": "/json-explorer/$1" 
    },
    {
      "handle": "filesystem"
    },
    { 
      "src": "/json-explorer/(.*).(js|css|ico|jpg|jpeg|png|gif|svg|webp)",
      "dest": "/json-explorer/$1.$2"
    },
    { 
      "src": "/json-explorer/(.*)", 
      "dest": "/json-explorer/index.html" 
    }
  ]
} 